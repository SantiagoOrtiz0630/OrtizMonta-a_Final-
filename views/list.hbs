<section id="up">

    <div id="slider">
        <div style="background-image: url('/rec/slide/0.jpg');"></div>
    </div>

    <header>
        <div class="logo">

        </div>

        <div class="menu">
            <a href="/">INICIO</a>
            <a href="/player/">CANCIONES</a>
            <a href="/list/">COLECCIÓN</a>
        </div>

    </header>

    <div class="cont">
        <a href="#listMusic">
            <div id="feedDown"></div>
        </a>
    </div>
</section>

<section id="list">
    <h2 class="titleSec">LISTA DE REPRODUCCIÓN</h2>
    <ul id="listMusic">
        {{#each musica}}
        <li class="songLink">
            <a href="/song/{{id}}#song">
                <div style="background-image: url('/rec/songs/album/{{album}}.jpg');"></div>
                <div class="cont">
                    <h2> {{nombre}} </h2>
                </div>
            </a>
        </li>
        {{/each}}
    </ul>
</section>

<footer>
    <div class="space"></div>
    <div class="cont">
        <div class="left">
            <ul>
                <li>Estados Unidos, NY</li>
                <li>The Neighbourhood - The NBHD</li>
            </ul>
        </div>
        <div class="right">
            <h2>Enlaces</h2>
            <ul>
                <li>
                    <a href="/">Inicio</a>
                </li>
                <li>
                    <a href="/player/">Canciones</a>
                </li>
                <li>
                    <a href="/list/">Colección</a>
                </li>
            </ul>
        </div>
    </div>
    <div class="feed">
        <a href="https://thenbhd.com/">www.thenbhd.com</a>
        <div class="cont">
            <a href="https://www.facebook.com/TheNeighbourhood/">
                <div style="background-image: url('rec/feed/1.svg')"></div>
            </a>
            <a href="https://www.youtube.com/user/TheNeighbourhoodVEVO">
                <div style="background-image: url('rec/feed/2.svg')"></div>
            </a>
            <a href="https://www.instagram.com/thenbhd/">
                <div style="background-image: url('rec/feed/3.svg')"></div>
            </a>
        </div>
    </div>
    <div class="space"></div>
</footer>

